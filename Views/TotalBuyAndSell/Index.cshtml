@model IEnumerable<Prod_Manger.Models.Domain.TotalBuyAndSellModel>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TotalGeneralBuyVat)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TotalGeneralBuyNoVAT)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TotalGeneralSellVAT)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TotalGeneralSellNoVAT)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TotalGeneralBuyVat)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TotalGeneralBuyNoVAT)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TotalGeneralSellVAT)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TotalGeneralSellNoVAT)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
